# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                            controllers.Application.login()
GET         /home                        controllers.Application.home()
GET         /help                        controllers.Application.help()
GET         /uebungen_oberkoerper        controllers.Application.oberkoerper()
GET         /studios                     controllers.Application.studios()
GET         /knstudio                    controllers.Application.knStudio()
GET         /uebungen_arme               controllers.Application.arme()
GET         /uebungen_beine              controllers.Application.beine()
GET         /uebungen_bauch              controllers.Application.bauch()
GET         /impressum                   controllers.Application.impressum()
GET         /kontakt                     controllers.Application.kontakt()
GET         /profil                      controllers.Application.profil()
GET         /plaene_anfaenger            controllers.Application.plaene_anfaenger()
GET         /plaene_fortgesch            controllers.Application.plaene_fortgesch()
GET         /plaene_profi                controllers.Application.plaene_profi()
GET         /addExercise                 controllers.Application.addExercise()
GET         /deleteExercise              controllers.Application.deleteExercise()
GET         /rateStudio                  controllers.Application.rateStudio()
GET         /logout                      controllers.Application.logout()
GET         /myStudio                    controllers.Application.myStudio()
POST        /createUser                  controllers.Application.createUser()
POST        /start                       controllers.Application.start()
POST        /changePassword              controllers.Application.changePassword()
GET         /sendMail                    controllers.Application.sendMail()
#AJAX
GET         /searchStudio                controllers.Application.searchStudio(value)
GET         /checkName                   controllers.Application.checkName(value)
GET         /checkPassword               controllers.Application.checkPassword(value)
# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                controllers.Assets.at(path="/public", file)
